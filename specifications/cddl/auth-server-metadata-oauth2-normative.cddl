  "authorization_endpoint" => url,
  "token_endpoint" => url,
  ? "jwks_uri" => url,
  ; ? "registration_endpoint" => url,
  ; ? "scopes_supported" => [ tstr ],
  "response_types_supported" => [ tstr ],
  ? "response_modes_supported" => [ tstr ],
  ; ? "grant_types_supported" => [ tstr ],
  ? "token_endpoint_auth_methods_supported" => oauth_token_endpoint_authentication_methods,
  ? "token_endpoint_auth_signing_alg_values_supported" => [ alg ],
