  ? "ui_locales_supported" => [ language-tag ],
  ; ? "op_policy_uri" => url,
  ; ? "op_tos_uri" => url,
  ; ? "revocation_endpoint" => url,
  ? "revocation_endpoint_auth_methods_supported" => [ oauth_token_endpoint_authentication_methods ],
  ? "revocation_endpoint_auth_signing_alg_values_supported" => [ alg ],
  ; ? "introspection_endpoint" => url,
  ? "introspection_endpoint_auth_methods_supported" => [ oauth_token_endpoint_authentication_methods ],
  ? "introspection_endpoint_auth_signing_alg_values_supported" => [ alg ],
